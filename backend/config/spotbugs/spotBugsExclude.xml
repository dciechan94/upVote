<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!-- spring injected fields null -->
    <Match>
        <Bug pattern="UWF_FIELD_NOT_INITIALIZED_IN_CONSTRUCTOR" />
    </Match>

    <!-- anonymous inner classes not allowed -->
    <Match>
        <Bug pattern="SIC_INNER_SHOULD_BE_STATIC_ANON" />
    </Match>

</FindBugsFilter>
